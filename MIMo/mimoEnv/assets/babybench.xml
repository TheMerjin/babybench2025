<mujoco model="MIMo">

    <!-- Import MIMo -->
	<worldbody>
        <!-- The location and orientation of the base model can be set using this body -->
		<body name="mimo_location" pos="0 0 .3" euler="0 -90 0">
			<freejoint name="mimo_location"/>
			<include file="mimo/MIMo_model.xml"></include> <!-- Import the actual model-->
		</body>
    </worldbody>

	<!-- Import everything else -->
    <include file="babybench/base.xml"></include>
	<include file="babybench/meta_v1.xml"></include>
	<include file="babybench/mimo_act.xml"></include>

</mujoco>
